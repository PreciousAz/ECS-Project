{
  "family": "culture_task_ec2",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::396913737331:role/culture_ecs_role",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "culture-container",
      "image": "<IMAGE_URI_REPLACED_BY_GITHUB_ACTIONS>",
      "cpu": 0,
      "essential": true,
      "environment": [],
      "portMappings": [
        {
          "name": "culture-port-name",
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/culture_task_ec2",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": [],
      "secrets": [
        {
          "name": "DB_HOST",
          "valueFrom": "arn:aws:ssm:us-east-1:396913737331:parameter/DB_HOST"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:ssm:us-east-1:396913737331:parameter/DB_NAME"
        },
        {
          "name": "DB_USER",
          "valueFrom": "arn:aws:ssm:us-east-1:396913737331:parameter/DB_USER"
        },
        {
          "name": "DB_PASS",
          "valueFrom": "arn:aws:ssm:us-east-1:396913737331:parameter/DB_PASS"
        },
        {
          "name": "DB_PORT",
          "valueFrom": "arn:aws:ssm:us-east-1:396913737331:parameter/DB_PORT"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:ssm:us-east-1:396913737331:parameter/JWT_SECRET"
        },
        {
          "name": "SESSION_SECRET",
          "valueFrom": "arn:aws:ssm:us-east-1:396913737331:parameter/SESSION_SECRET"
        }
      ]
    }
  ]
}
